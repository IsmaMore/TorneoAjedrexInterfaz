<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="fondo" alignment="CENTER" prefHeight="600.0" prefWidth="900.0" spacing="20.0" stylesheets="@estilo1.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.torneoloquesea.InicioController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Label prefHeight="59.0" prefWidth="654.0" text="BENIDORM CHESS OPEN" textAlignment="RIGHT" textFill="WHITE">
      <font>
         <Font name="System Bold Italic" size="48.0" />
      </font>
   </Label>
   <VBox alignment="BOTTOM_RIGHT" prefHeight="632.0" prefWidth="830.0">
      <children>
         <Button fx:id="btmIniciar" mnemonicParsing="false" onAction="#entrarTorneo" prefHeight="70.0" prefWidth="150.0" style="-fx-background-radius: 10;" text="Iniciar" textAlignment="CENTER">
            <font>
               <Font name="System Bold Italic" size="18.0" />
            </font>
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor>
            <VBox.margin>
               <Insets bottom="20.0" />
            </VBox.margin></Button>
        <Button fx:id="btmsalida" onAction="#salir" prefHeight="50.0" prefWidth="150.0" style="-fx-background-radius: 10;" text="Exit">
            <font>
               <Font name="System Bold" size="18.0" />
            </font>
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Button>
      </children>
   </VBox>
</VBox>
