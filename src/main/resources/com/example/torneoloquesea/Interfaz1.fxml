<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="fondo" alignment="CENTER" prefHeight="600.0" prefWidth="900.0" spacing="20.0" stylesheets="@estilo1.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.torneoloquesea.InicioController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Label text="Benidorm Chess Open" textFill="#cdbd2e">
      <font>
         <Font name="System Bold Italic" size="29.0" />
      </font>
   </Label>
   <HBox alignment="CENTER" prefHeight="579.0" prefWidth="860.0" spacing="80.0">
      <children>
         <Button fx:id="btmIniciar" mnemonicParsing="false" onAction="#entrarTorneo" prefHeight="70.0" prefWidth="150.0" style="-fx-background-color: white;" text="Iniciar">
            <font>
               <Font name="System Bold Italic" size="18.0" />
            </font></Button>
      </children>
      <VBox.margin>
         <Insets />
      </VBox.margin>
   </HBox>
   <HBox alignment="BOTTOM_RIGHT" prefHeight="416.0" prefWidth="849.0">
      <children>
        <Button fx:id="btmsalida" onAction="#salir" prefHeight="50.0" prefWidth="92.0" style="-fx-background-color: black; -fx-border-color: grey; -fx-border-width: 2;" text="Exit" textFill="#fffdfd">
            <font>
               <Font name="System Bold" size="13.0" />
            </font></Button>
      </children>
   </HBox>
</VBox>
